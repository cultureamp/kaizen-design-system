{
  "private": true,
  "workspaces": [
    "packages/*",
    "draft-packages/*",
    "legacy-packages/*"
  ],
  "engines": {
    "node": ">=14.18.0"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006 -c storybook",
    "storybook:build": "build-storybook -c storybook -o storybook/public",
    "eslint": "eslint . -c .eslintrc.js --ext .ts,.tsx",
    "prettier": "prettier --check '**/*'",
    "stylelint": "stylelint '**/*.scss'",
    "test": "jest",
    "jest:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --no-cache",
    "compile": "tsc",
    "lint": "yarn eslint --max-warnings=0 && yarn prettier && yarn stylelint",
    "lint:fix": "yarn stylelint --fix && yarn eslint --max-warnings=0 --fix && yarn prettier --write",
    "clean:storybook": "rimraf 'storybook/public'",
    "clean": "lerna run clean && yarn clean:storybook",
    "reset": "yarn clean && yarn install --force",
    "chromatic": "chromatic",
    "commit": "cz"
  },
  "dependencies": {
    "@storybook/addons": "^6.5.9",
    "@storybook/api": "^6.5.9",
    "@storybook/components": "^6.5.9",
    "@storybook/core-events": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/theming": "^6.5.9",
    "chalk": "^2.4.2",
    "classnames": "^2.3.1",
    "focus-visible": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "lodash.isempty": "^4.4.0",
    "node-sass": "^7.0.1",
    "normalize.css": "^8.0.1",
    "postcss-flexbugs-fixes": "^4.2.1",
    "react": "^18.1.0",
    "react-16": "npm:react@^16.9.0",
    "react-17": "npm:react@^17.0.2",
    "react-dom": "^18.1.0",
    "react-dom-16": "npm:react-dom@^16.9.0",
    "react-dom-17": "npm:react-dom@^17.0.2",
    "react-gtm-module": "^2.0.11",
    "svgo": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-numeric-separator": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-a11y": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.6",
    "@storybook/manager-webpack5": "^6.5.6",
    "@storybook/node-logger": "^6.5.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/react-12": "npm:@testing-library/react@^12.1.5",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/chalk": "^2.2.0",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.29",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.21.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "^8.2.5",
    "chromatic": "^6.2.3",
    "commitizen": "^4.2.4",
    "css-loader": "^3.6.0",
    "cz-conventional-changelog": "3.3.0",
    "danger": "^11.0.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-ssr-friendly": "^1.0.6",
    "eslint-plugin-storybook": "^0.5.12",
    "jest": "^27.5.1",
    "jest-canvas-mock": "^2.3.1",
    "jest-circus": "^27.4.2",
    "jest-static-stubs": "^0.0.1",
    "lerna": "3.22.1",
    "node-fetch": "^2.6.7",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.1",
    "prettier": "^2.5.1",
    "raw-loader": "^4.0.2",
    "react-test-renderer": "^18.1.0",
    "react-test-renderer-17": "npm:react-test-renderer@^17.0.2",
    "sass-loader": "^12.6.0",
    "storybook-addon-designs": "^6.2.1",
    "style-loader": "^1.3.0",
    "stylelint": "^13.13.1",
    "svg-sprite-loader": "^6.0.11",
    "svgo-loader": "^2.2.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.5.5",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.68.0"
  },
  "resolutions": {
    "@types/react": "^18.0.9"
  },
  "resolutionsComments": {
    "@types/react": "@testing-library/react-12 has an incorrect dep (instead of peerDep) of @types/react@^17 which we don't want"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
