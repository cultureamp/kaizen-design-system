s, opts) {\n    super(args, opts)\n  }\n\n  async prompting() {\n    this.answers = await this.prompt([\n      {\n        type: \"input\",\n        name: \"name\",\n        message: \"Your component name in PascalCase (Eg: EmptyState)\",\n        default: \"\",\n      },\n      {\n        type: \"confirm\",\n        name: \"elm\",\n        message: \"Would you like to generate Elm?\",\n      },\n      {\n        type: \"confirm\",\n        name: \"react\",\n        message: \"Would you like to generate React?\",\n      },\n    ])\n\n    this.log(\"app name\", this.answers.name)\n  }\n\n  writing() {\n    const componentName = pascalCase(this.answers.name)\n    const packageLocation = `draft-packages/${kebabCase(this.answers.name)}/`\n    // root level files\n    this.fs.copyTpl(\n      this.templatePath(\"package.txt\"),\n      this.destinationPath(`${packageLocation}package.json`),\n      { componentName: kebabCase(this.answers.name) }\n    )\n\n    this.fs.copyTpl(\n      this.templatePath(\"tsconfig.txt\"),\n      this.destinationPath(`${packageLocation}tsconfig.dist.json`)\n    )\n\n    // elm component files\n    if (this.answers.elm) {\n      this.fs.copyTpl(\n        this.templatePath(\"component-elm.txt\"),\n        this.destinationPath(\n          `${packageLocation}KaizenDraft/${componentName}/${componentName}.elm`\n        ),\n        { componentName }\n      )\n    }\n\n    // react component files\n    if (this.answers.react) {\n      this.fs.copyTpl(\n        this.templatePath(\"index.txt\"),\n        this.destinationPath(`${packageLocation}index.ts`),\n        { componentName }\n      )\n\n      this.fs.copyTpl(\n        this.templatePath(\"component-react.txt\"),\n        this.destinationPath(\n          `${packageLocation}/KaizenDraft/${componentName}/${componentName}.tsx`\n        ),\n        { componentName }\n      )\n\n      this.fs.copyTpl(\n        this.templatePath(\"story-react.txt\"),\n        this.destinationPath(\n          `draft-packages/stories/${componentName}.stories.tsx`\n        ),\n        {\n          componentName: pascalCase(this.answers.name),\n          npmPackageName: `@kaizen/draft-${kebabCase(this.answers.name)}`,\n        }\n      )\n    }\n\n    // scss files\n    this.fs.copyTpl(\n      this.templatePath(\"component-scss.txt\"),\n      this.destinationPath(\n        `${packageLocation}/KaizenDraft/${componentName}/styles.module.scss`\n      )\n    )\n  }\n}\n\nfunction pascalCase(s) {\n  return startCase(s).replace(/ /gi, \"\")\n}\n","/Users/ally.beaton/Code/kds/packages/hosted-assets/index.ts",[],"/Users/ally.beaton/Code/kds/site/src/components/ContentMarkdownSection.tsx",[],"/Users/ally.beaton/Code/kds/site/src/components/ContentOnly.tsx",[],"/Users/ally.beaton/Code/kds/site/src/components/Footer.tsx",[],"/Users/ally.beaton/Code/kds/site/src/components/Head.tsx",[],"/Users/ally.beaton/Code/kds/site/src/components/Layout.tsx",[],"/Users/ally.beaton/Code/kds/site/src/components/MainNav.tsx",[],"/Users/ally.beaton/Code/kds/site/src/components/PageHeader.tsx",[],"/Users/ally.beaton/Code/kds/site/src/components/SidebarAndContent.tsx",[],"/Users/ally.beaton/Code/kds/site/src/components/StorybookDemo.tsx",[],"/Users/ally.beaton/Code/kds/site/src/components/Tag.tsx",[],"/Users/ally.beaton/Code/kds/site/src/components/markdownComponents.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/Card.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/Code.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/Do.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/DoAndDontContainer.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/DoOrDontItem.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/Dont.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/Link.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/WhenNotToUse.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/WhenOrWhenNotToUse.tsx",[],"/Users/al