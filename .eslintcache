packages/component-library/components/Button/IconButton.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Button/components/GenericButton.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Button/index.ts",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Dropdown/Dropdown.spec.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Dropdown/Dropdown.tsx",["1544","1545"],"import { Icon } from \"@kaizen/component-library\"\nconst chevronDownIcon = require(\"@kaizen/component-library/icons/chevron-down.icon.svg\")\n  .default\nconst ellipsisIcon = require(\"@kaizen/component-library/icons/ellipsis.icon.svg\")\n  .default\nimport classNames from \"classnames\"\nimport * as React from \"react\"\nimport DropdownMenu from \"./DropdownMenu\"\n\nconst styles = require(\"./Dropdown.module.scss\")\n\ntype DropdownState = {\n  isMenuVisible: boolean\n}\n\nexport type DropdownProps = {\n  icon?: React.SVGAttributes<SVGSymbolElement>\n  label?: React.ReactNode\n  menuVisible?: boolean\n  controlAction?: boolean\n  automationId?: string\n  reversedColor?: boolean\n  iconPosition?: \"start\" | \"end\"\n}\n\nexport default class Dropdown extends React.Component<\n  DropdownProps,\n  DropdownState\n> {\n  static displayName = \"Dropdown\"\n  static defaultProps = {\n    iconPosition: \"start\",\n  }\n\n  dropdownButton = React.createRef<HTMLButtonElement>()\n\n  constructor(props: DropdownProps) {\n    super(props)\n\n    this.state = {\n      isMenuVisible: Boolean(props.menuVisible),\n    }\n  }\n\n  toggleDropdownMenu = (e: React.SyntheticEvent<HTMLButtonElement>) => {\n    e.stopPropagation()\n\n    const currentState = this.state.isMenuVisible\n    this.setState({\n      isMenuVisible: !currentState,\n    })\n  }\n\n  hideDropdownMenu = () => {\n    this.setState({\n      isMenuVisible: false,\n    })\n  }\n\n  getPosition() {\n    return this.dropdownButton && this.dropdownButton.current\n      ? this.dropdownButton.current.getBoundingClientRect()\n      : null\n  }\n\n  renderDropdownMenu() {\n    return (\n      <DropdownMenu\n        hideDropdownMenu={this.hideDropdownMenu}\n        position={this.getPosition()}\n      >\n        {this.props.children}\n      </DropdownMenu>\n    )\n  }\n\n  renderIcon = (icon?: React.SVGAttributes<SVGSymbolElement>) => {\n    if (!icon) return\n\n    return (\n      <span className={styles.dropdownIcon}>\n        <Icon icon={icon} role=\"img\" title=\"Open menu\" />\n      </span>\n    )\n  }\n\n  renderDownArrow = () => {\n    const { label, controlAction } = this.props\n    if (!label || !controlAction) return\n\n    return (\n      <span className={styles.chevronIcon}>\n        <Icon icon={chevronDownIcon} role=\"img\" title=\"Open menu\" />\n      </span>\n    )\n  }\n\n  renderButtonContent = () => {\n    const { label } = this.props\n    let { icon } = this.props\n\n    if (!icon && !label) {\n      icon = ellipsisIcon\n    }\n\n    return (\n      <React.Fragment>\n        {this.renderIcon(icon)}\n        {label && <span className={styles.dropdownLabel}>{label}</span>}\n        {this.renderDownArrow()}\n      </React.Fragment>\n    )\n  }\n\n  renderReversedButtonContent = () => {\n    const { icon, label } = this.props\n\n    return (\n      <React.Fragment>\n        {this.renderDownArrow()}\n        {label && <span className={styles.dropdownLabel}>{label}</span>}\n        {this.renderIcon(icon)}\n      </React.Fragment>\n    )\n  }\n\n  render() {\n    const {\n      controlAction,\n      automationId,\n      iconPosition,\n      reversedColor,\n    } = this.props\n\n    const reverseIcon = iconPosition === \"end\"\n    const btnClass = classNames(styles.dropdownButton, {\n      [styles.dropdownControlAction]: controlAction,\n      [styles.isOpen]: this.state.isMenuVisible,\n      [styles.reversedColor]: reversedColor,\n    })\n    return (\n      <div className={styles.dropdown}>\n        <button\n          className={btnClass}\n          onClick={this.toggleDropdownMenu}\n          onMouseDown={e => e.preventDefault()}\n          ref={this.dropdownButton}\n          data-automation-id={automationId}\n        >\n          {!reverseIcon && this.renderButtonContent()}\n          {reverseIcon && this.renderReversedButtonContent()}\n        </button>\n        {this.state.isMenuVisible && this.renderDropdownMenu()}\n      </div>\n    )\n  }\n}\n","/Users/ally.beaton/Code/kds/packages/component-library/components/Dropdown/DropdownMenu.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Dropdown/index.ts",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Heading/Heading.spec.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Heading/Heading.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Heading/index.ts",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Icon/Icon.spec.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Icon/Icon.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Icon/index.ts",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Layout/Layout.spec.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Layout/Layout.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Layout/index.ts",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/MenuList/MenuHeader.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/MenuList/MenuItem.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/MenuList/MenuList.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/MenuList/MenuSeparator.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/MenuList/index.ts",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/NavigationBar/NavigationBar.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/NavigationBar/components/Badge.tsx",["1546","1547"],"import { Icon } from \"@kaizen/component-library\"\nconst caMonogramIcon = require(\"@kaizen/component-library/icons/ca-monogram.icon.svg\")\n  .default\nconst spinnerIcon = require(\"@kaizen/component-library/icons/spinner.icon.svg\")\n  .default\nimport classNames from \"classnames\"\nimport * as React from \"react\"\n\nconst styles = require(\"./Badge.module.scss\")\n\ntype BadgeProps = {\n  loading: boolean\n  href: string\n  colorScheme: string\n}\n\nexport const ProductionBadge = (props: BadgeProps) => (\n  <div className={classNames(styles.badge, styles[props.colorScheme])}>\n    <a href={props.href}>\n      {props.loading ? (\n        <Icon icon={spinnerIcon} title=\"loading\" />\n      ) : (\n        <Icon icon={caMonogramIcon} title=\"Culture Amp\" />\n      )}\n    </a>\n  </div>\n)\n\nProductionBadge.displayName = \"ProductionBadge\"\n\nexport const StagingBadge = (props: BadgeProps) => (\n  <MonogramBadge\n    envClass={styles.staging}\n    monogram=\"staging\"\n    loading={props.loading}\n    href={props.href}\n    colorScheme={props.colorScheme}\n  />\n)\n\nStagingBadge.displayName = \"StagingBadge\"\n\nexport const TestBadge = (props: BadgeProps) => (\n  <MonogramBadge\n    envClass={styles.test}\n    monogram=\"test\"\n    loading={props.loading}\n    href={props.href}\n    colorScheme={props.colorScheme}\n  />\n)\n\nTestBadge.displayName = \"TestBadge\"\n\nexport const LocalBadge = (props: BadgeProps) => (\n  <MonogramBadge\n    envClass={styles.local}\n    monogram=\"local\"\n    loading={props.loading}\n    href={props.href}\n    colorScheme={props.colorScheme}\n  />\n)\n\nLocalBadge.displayName = \"LocalBadge\"\n\nexport const namedBadge = (environment: string) => (props: BadgeProps) => (\n  <MonogramBadge\n    envClass={styles.named}\n    monogram={environment}\n    loading={props.loading}\n    href={props.href}\n    colorScheme={props.colorScheme}\n  />\n)\n\nnamedBadge.displayName = \"namedBadge\"\n\ntype MonogramBadgeProps = BadgeProps & {\n  envClass: string\n  monogram: string\n}\n\nconst MonogramBadge = (props: MonogramBadgeProps) => (\n  <div className={classNames(styles.badge, props.envClass)}>\n    <a href={props.href} aria-live=\"polite\">\n      {props.loading ? (\n        <Icon icon={spinnerIcon} title=\"loading\" />\n      ) : (\n        <span aria-label={props.monogram}>{props.monogram.substr(0, 2)}</span>\n      )}\n    </a>\n  </div>\n)\n\nMonogramBadge.displayName = \"MonogramBadge\"\n","/Users/ally.beaton/Code/kds/packages/component-library/components/NavigationBar/components/Link.tsx",["1548"],"import { Icon } from \"@kaizen/component-library\"\nconst chevronRightIcon = require(\"@kaizen/component-library/icons/chevron-right.icon.svg\")\n  .default\nimport classNames from \"classnames\"\nimport * as React from \"react\"\n\nconst styles = require(\"./Link.module.scss\")\nimport { LinkProps } from \"../types\"\n\nexport default class Link extends React.PureComponent<LinkProps> {\n  static displayName = \"Link\"\n  static defaultProps = {\n    iconOnly: false,\n    active: false,\n    secondary: false,\n    new: false,\n    target: \"_self\",\n  }\n\n  render = () => {\n    const {\n      badge,\n      icon,\n      text,\n      href,\n      active,\n      id,\n      onClick,\n      iconOnly,\n      target,\n      hasMenu,\n      section,\n    } = this.props\n\n    return (\n      <a\n        className={classNames(styles.link, {\n          [styles.active]: active,\n          [styles.containsText]: text !== \"\",\n          [styles.secondary]: section === \"secondary\",\n        })}\n        tabIndex={0}\n        {...{ href, id, onClick, target }}\n      >\n        {icon && (\n          <span className={styles.linkIcon}>\n            <Icon\n              icon={icon}\n              role={iconOnly ? \"img\" : \"presentation\"}\n              title={iconOnly ? text : undefined}\n            />\n          </span>\n        )}\n        {text && !(icon && iconOnly) && (\n          <span className={styles.linkText}>\n            {text}\n            {badge && (\n              <span\n                className={classNames(styles.badge, {\n                  [styles.badgeNotification]: badge.kind === \"notification\",\n                  [styles.badgeNew]: badge.kind === \"new\",\n                })}\n              >\n                {badge.text}\n              </span>\n            )}\n          </span>\n        )}\n        {hasMenu && (\n          <span className={styles.menuIcon}>\n            <Icon icon={chevronRightIcon} role=\"presentation\" />\n          </span>\n        )}\n      </a>\n    )\n  }\n}\n","/Users/ally.beaton/Code/kds/packages/component-library/components/NavigationBar/components/Menu.tsx",["1549","1550"],"import {\n  Icon,\n  IconButton,\n  OffCanvas,\n  OffCanvasContext,\n} from \"@kaizen/component-library\"\nconst arrowLeftIcon = require(\"@kaizen/component-library/icons/arrow-left.icon.svg\")\n  .default\n\nconst chevronDownIcon = require(\"@kaizen/component-library/icons/chevron-down.icon.svg\")\n  .default\nimport classNames from \"classnames\"\nimport * as React from \"react\"\nimport Media from \"react-media\"\nimport { MOBILE_QUERY } from \"../constants\"\nimport { MenuGroup, MenuItem, MenuProps } from \"../types\"\nimport Link from \"./Link\"\n\nconst styles = require(\"./Menu.module.scss\")\n\ntype State = {\n  open: boolean\n}\n\nexport default class Menu extends React.Component<MenuProps, State> {\n  static displayName = \"Menu\"\n  static defaultProps = {\n    items: [],\n    active: false,\n    mobileEnabled: true,\n  }\n  rootRef = React.createRef<any>()\n\n  state = { open: false }\n\n  render() {\n    const {\n      active,\n      children,\n      automationId,\n      heading,\n      mobileEnabled,\n      section,\n    } = this.props\n\n    return (\n      <Media query={MOBILE_QUERY}>\n        {(matches: boolean) =>\n          mobileEnabled && matches ? (\n            <React.Fragment>\n              <OffCanvasContext.Consumer>\n                {({ toggleVisibleMenu }) => (\n                  <Link\n                    text={heading}\n                    href=\"#\"\n                    onClick={() => toggleVisibleMenu(heading)}\n                    hasMenu\n                    section={section}\n                  />\n                )}\n              </OffCanvasContext.Consumer>\n              {this.renderOffCanvas()}\n            </React.Fragment>\n          ) : (\n            <nav className={styles.root} ref={this.rootRef}>\n              <button\n                className={classNames(styles.button, {\n                  [styles.buttonLink]: section === \"primary\",\n                  [styles.active]: active,\n                  [styles.linkText]: !!heading,\n                })}\n                onClick={this.toggle}\n                aria-expanded={this.state.open}\n                data-automation-id={automationId}\n                onMouseDown={e => e.preventDefault()}\n              >\n                {children ? (\n                  children\n                ) : (\n                  <React.Fragment>\n                    <span className={styles.linkText}>{heading}</span>\n                    <Icon icon={chevronDownIcon} role=\"presentation\" />\n                  </React.Fragment>\n                )}\n              </button>\n              {this.state.open && this.renderMenu()}\n            </nav>\n          )\n        }\n      </Media>\n    )\n  }\n\n  toggle = (\n    e: React.SyntheticEvent<HTMLAnchorElement | HTMLButtonElement> | MouseEvent\n  ) => {\n    const open = !this.state.open\n    this.setState({ open })\n  }\n\n  renderMenu() {\n    const { header, items } = this.props\n\n    return (\n      <div className={styles.menu}>\n        <div>\n          {header}\n          {items.map(item => {\n            if (\"url\" in item) {\n              return this.renderMenuItem(item)\n            } else if (\"title\" in item) {\n              return this.renderMenuGroup(item)\n            }\n          })}\n        </div>\n      </div>\n    )\n  }\n\n  renderOffCanvas() {\n    const { items, heading } = this.props\n    const links = items.map(item => {\n      if (\"url\" in item) {\n        return this.renderOffCanvasMenuItem(item)\n      } else if (\"title\" in item) {\n        return this.renderOffCanvasMenuGroup(item)\n      }\n    })\n\n    return (\n      <OffCanvas\n        links={links}\n        heading={heading ? heading : \"Menu\"}\n        headerComponent={this.renderBackButton()}\n        menuId={heading}\n      />\n    )\n  }\n\n  renderBackButton() {\n    return (\n      <OffCanvasContext.Consumer>\n        {({ toggleVisibleMenu }) => (\n          <IconButton\n            label=\"Back\"\n            icon={arrowLeftIcon}\n            onClick={() => toggleVisibleMenu(this.props.heading)}\n            reversed\n          />\n        )}\n      </OffCanvasContext.Consumer>\n    )\n  }\n\n  renderOffCanvasMenuItem = (item: MenuItem) => {\n    const { onLinkClick } = this.props\n    return (\n      <Link\n        key={item.url}\n        text={item.label}\n        href={item.url}\n        onClick={onLinkClick}\n      />\n    )\n  }\n  renderOffCanvasMenuGroup = (menuGroup: MenuGroup) => {\n    const { title, items } = menuGroup\n\n    return (\n      <div className={styles.offCanvasMenuGroup}>\n        <h4 className={styles.offCanvasMenuGroupTitle}>{title}</h4>\n        {items.map(this.renderOffCanvasMenuItem)}\n      </div>\n    )\n  }\n\n  renderMenuItem = (item: MenuItem) => {\n    const { onLinkClick } = this.props\n    const { label, url, method, active = false } = item\n\n    if (method && method !== \"get\") {\n      return (\n        // HTML forms only accept POST. We use a hidden `_method` input as a convention for emulating other HTTP verbs.\n        // This behaviour is the same as what is implemented by UJS and supported by Rails:\n        // https://github.com/rails/jquery-ujs\n        <form method=\"post\" action={url}>\n          <input name=\"_method\" value={method} type=\"hidden\" />\n          <button\n            type=\"submit\"\n            className={classNames(styles.menuItem, {\n              [styles.menuItemActive]: active,\n            })}\n          >\n            {label}\n          </button>\n        </form>\n      )\n    }\n\n    return (\n      <a\n        href={url}\n        className={classNames(styles.menuItem, {\n          [styles.menuItemActive]: active,\n        })}\n        tabIndex={0}\n        onClick={onLinkClick}\n      >\n        {label}\n      </a>\n    )\n  }\n\n  renderMenuGroup = (menuGroup: MenuGroup) => {\n    const { title, items } = menuGroup\n\n    return (\n      <div className={styles.menuGroup}>\n        <h4 className={styles.menuGroupTitle}>{title}</h4>\n        {items.map(this.renderMenuItem)}\n      </div>\n    )\n  }\n\n  componentDidMount() {\n    document.addEventListener(\"click\", this.clickDocument)\n  }\n\n  componentWillUnmount() {\n    document.removeEventListener(\"click\", this.clickDocument)\n  }\n\n  clickDocument = (e: MouseEvent) => {\n    // We can't just stopPropagation of click events in the menu, because a\n    // click in this menu may also need to dismiss another open menu.\n    if (\n      this.state.open &&\n      this.rootRef.current &&\n      !(e.target instanceof Node && this.rootRef.current.contains(e.target))\n    ) {\n      this.toggle(e)\n    }\n  }\n}\n","/Users/ally.beaton/Code/kds/packages/component-library/components/NavigationBar/constants.ts",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/NavigationBar/index.ts",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/NavigationBar/types.ts",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Notification/GlobalNotification.spec.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Notification/GlobalNotification.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Notification/InlineNotification.spec.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Notification/InlineNotification.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Notification/ToastNotification.spec.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Notification/ToastNotification.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Notification/components/GenericNotification.spec.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/Notification/components/GenericNotification.tsx",["1551","1552","1553","1554"],"import { Icon } from \"@kaizen/component-library\"\nconst closeIcon = require(\"@kaizen/component-library/icons/close.icon.svg\")\n  .default\nconst exclamationIcon = require(\"@kaizen/component-library/icons/exclamation.icon.svg\")\n  .default\nconst informationIcon = require(\"@kaizen/component-library/icons/information.icon.svg\")\n  .default\nconst successIcon = require(\"@kaizen/component-library/icons/success.icon.svg\")\n  .default\nimport classnames from \"classnames\"\nimport * as React from \"react\"\n\nconst styles = require(\"./GenericNotification.module.scss\")\n\nexport type NotificationType =\n  | \"affirmative\"\n  | \"informative\"\n  | \"cautionary\"\n  | \"negative\"\n\ntype Props = {\n  type: NotificationType\n  style: \"global\" | \"inline\" | \"toast\"\n  children: React.ReactNode\n  title?: string\n  persistent: boolean\n  autohide: boolean\n  autohideDelay?: \"short\" | \"long\"\n  onHide?: () => void\n  automationId?: string\n  noBottomMargin?: boolean\n}\n\ntype State = {\n  hidden: boolean\n  removed: boolean\n}\n\nclass GenericNotification extends React.Component<Props, State> {\n  static defaultProps = {\n    persistent: false,\n    autohide: false,\n    autohideDelay: \"short\",\n  }\n\n  state = {\n    hidden: true,\n    removed: false,\n  }\n  containerRef = React.createRef<HTMLDivElement>()\n\n  constructor(props: Props) {\n    super(props)\n\n    this.hide = this.hide.bind(this)\n    this.onTransitionEnd = this.onTransitionEnd.bind(this)\n  }\n\n  componentDidMount() {\n    requestAnimationFrame(() => this.setState({ hidden: false }))\n\n    if ([\"toast\", \"inline\"].includes(this.props.style) && this.props.autohide) {\n      setTimeout(this.hide, this.autohideDelayMs())\n    }\n  }\n\n  autohideDelayMs() {\n    if (this.props.autohideDelay == \"long\") {\n      return 30_000\n    } else {\n      return 5_000\n    }\n  }\n\n  render() {\n    if (this.state.removed) {\n      return null\n    }\n\n    return (\n      <div\n        className={this.className()}\n        style={{ marginTop: this.marginTop() }}\n        ref={this.containerRef}\n        onTransitionEnd={this.onTransitionEnd}\n        data-automation-id={this.props.automationId}\n      >\n        <div className={styles.icon}>\n          <Icon icon={this.iconType()} role=\"presentation\" inheritSize />\n        </div>\n        <div className={styles.textContainer}>\n          {this.props.title && (\n            <h6 className={styles.title}>{this.props.title}</h6>\n          )}\n          <p className={styles.text}>{this.props.children}</p>\n        </div>\n        {!this.props.persistent && <CancelButton onClick={this.hide} />}\n      </div>\n    )\n  }\n\n  className(): string {\n    return classnames(\n      styles.notification,\n      styles[this.props.type],\n      styles[this.props.style],\n      {\n        [styles.hidden]: this.state.hidden,\n        [styles.noBottomMargin]: this.props.noBottomMargin,\n      }\n    )\n  }\n\n  marginTop(): string {\n    if (this.state.hidden && this.containerRef.current) {\n      return -this.containerRef.current.clientHeight + \"px\"\n    }\n\n    return \"0\"\n  }\n\n  onTransitionEnd(e: React.TransitionEvent<HTMLDivElement>) {\n    // Be careful: this assumes the final CSS property to be animated is \"margin-top\".\n    if (this.state.hidden && e.propertyName === \"margin-top\") {\n      this.setState({ removed: true })\n      if (this.props.onHide) {\n        this.props.onHide()\n      }\n    }\n  }\n\n  iconType() {\n    switch (this.props.type) {\n      case \"affirmative\":\n        return successIcon\n      case \"negative\":\n        return exclamationIcon\n      case \"cautionary\":\n        return exclamationIcon\n      case \"informative\":\n        return informationIcon\n      default:\n        return informationIcon\n    }\n  }\n\n  hide() {\n    this.setState({ hidden: true })\n  }\n}\n\ntype CancelButtonProps = {\n  onClick: (e: React.MouseEvent<HTMLButtonElement>) => void\n}\n\nconst CancelButton = ({ onClick }: CancelButtonProps) => (\n  <button className={styles.cancel} type=\"button\" onClick={onClick}>\n    <span className={styles.cancelInner}>\n      <Icon icon={closeIcon} role=\"img\" title=\"close notification\" />\n    </span>\n  </button>\n)\n\nexport default GenericNotification\n","/Users/ally.beaton/Code/kds/packages/component-library/components/Notification/index.ts",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/OffCanvas/OffCanvas.tsx",[],"/Users/ally.beaton/Code/kds/packages/component-library/components/OffCanvas/components/Header.tsx",["1555"],"const closeIcon = require(\"@kaizen/component-library/icons/close.icon.svg\")\n  .default\nimport * as React from \"react\"\nimport IconButton from \"../../Button/IconButton\"\n\nconst styles = require(\"./Header.module.scss\")\n\ntype Props = {\n  leftComponent: React.ReactNode\n  onClose: (e: MouseEvent) => void\n  heading: string\n}\n\nconst Header = ({ leftComponent, onClose, heading }: Props) => (\n  <div className={styles.root}>\n    {leftComponent}\n    <span className={styles.heading}>{heading}</span>\n    <IconButton label=\"Close\" icon={closeIcon} onClick={onClose} reversed />\n  </div>\n)\n\nexport d