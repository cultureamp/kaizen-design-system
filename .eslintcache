moveAriaHider()\n  }\n\n  addEventHandlers() {\n    this.props.onEscapeKeyup &&\n      document.addEventListener(\"keyup\", this.escapeKeyHandler)\n  }\n\n  removeEventHandlers() {\n    this.props.onEscapeKeyup &&\n      document.removeEventListener(\"keyup\", this.escapeKeyHandler)\n  }\n\n  // eslint-disable-next-line @typescript-eslint/no-empty-function\n  removeAriaHider(): void {}\n\n  preventBodyScroll() {\n    const hasScrollbar =\n      window.innerWidth > document.documentElement.clientWidth\n    document.documentElement.classList.add(\n      ...[styles.unscrollable, hasScrollbar && styles.pseudoScrollbar]\n    )\n  }\n\n  restoreBodyScroll() {\n    document.documentElement.classList.remove(\n      styles.unscrollable,\n      styles.pseudoScrollbar\n    )\n  }\n\n  escapeKeyHandler = (event: KeyboardEvent) => {\n    if (\n      event.key === \"Escape\" ||\n      event.key === \"Esc\" // IE11\n    )\n      this.props.onEscapeKeyup && this.props.onEscapeKeyup(event)\n  }\n\n  outsideModalClickHandler = (event: React.MouseEvent) => {\n    if (event.target === this.scrollLayer || event.target === this.modalLayer) {\n      this.props.onOutsideModalClick && this.props.onOutsideModalClick(event)\n    }\n  }\n\n  ensureAccessiblityIsMet() {\n    if (!this.modalLayer) return\n    // Ensure that consumers have provided an element that labels the modal\n    // to meet ARIA accessibility guidelines.\n    if (!document.getElementById(this.props.labelledByID)) {\n      warn(\n        `When using the Modal component, you must provide a label for the modal.\n        Make sure you have a <ModalAccessibleLabel /> component with content that labels the modal.`\n      )\n    }\n  }\n\n  scrollModalToTop() {\n    // If we have a really long modal, the autofocus could land on an element down below\n    // causing the modal to scroll down and skipping over the content near the modal's top.\n    // Ensure that when the modal opens, we are at the top of its content.\n    requestAnimationFrame(() => {\n      if (!this.scrollLayer) return\n      this.scrollLayer.scrollTop = 0\n    })\n  }\n\n  focusAccessibleLabel() {\n    if (this.modalLayer) {\n      const labelElement: HTMLElement | null = document.getElementById(\n        this.props.labelledByID\n      )\n      if (labelElement) {\n        labelElement.focus()\n      }\n    }\n  }\n\n  render(): React.ReactPortal {\n    const {\n      isOpen,\n      children,\n      focusLockDisabled = false,\n      automationId,\n    } = this.props\n\n    return createPortal(\n      <CSSTransition\n        in={isOpen}\n        timeout={MODAL_TRANSITION_TIMEOUT}\n        classNames=\"animating\"\n        appear={true}\n        unmountOnExit\n      >\n        {/* This is not an unused div. It will receive `animating-` classes from react-transition-group */}\n        <div>\n          <FocusLock\n            disabled={focusLockDisabled}\n            returnFocus={true}\n            autoFocus={false}\n          >\n            <div className={styles.backdropLayer} />\n            <div\n              className={styles.scrollLayer}\n              ref={scrollLayer => (this.scrollLayer = scrollLayer)}\n              onClick={\n                this.props.onOutsideModalClick && this.outsideModalClickHandler\n              }\n            >\n              <div\n                role=\"dialog\"\n                aria-labelledby={this.props.labelledByID}\n                aria-describedby={this.props.describedByID}\n                className={styles.modalLayer}\n                ref={modalLayer => (this.modalLayer = modalLayer)}\n                data-automation-id={automationId}\n              >\n                {children}\n              </div>\n            </div>\n          </FocusLock>\n        </div>\n      </CSSTransition>,\n      document.body\n    )\n  }\n}\n\n/**\n * Get an element's owner document. Useful when components are used in iframes\n * or other environments like dev tools.\n */\nfunction getOwnerDocument<T extends HTMLElement = HTMLElement>(\n  element: T | null\n) {\n  return element && element.ownerDocument\n    ? element.ownerDocument\n    : canUseDOM()\n    ? document\n    : null\n}\n\n/**\n * Check if the DOM exists and is usable\n */\nfunction canUseDOM(): boolean {\n  return (\n    typeof window !== \"undefined\" &&\n    typeof window.document !== \"undefined\" &&\n    typeof window.document.createElement !== \"undefined\"\n  )\n}\n\n// eslint-disable-next-line @typescript-eslint/no-empty-function\nfunction noop(): void {}\n\n/**\n * Hide elements in the DOM from screenreaders that are outside the parent tree\n * of the reference node. We do this by setting `aria-hidden` attribute to true\n * for any elements top-level DOM nodes.\n *\n * Returns a function that restores the _original_ values of the affected nodes,\n * so any pre-aria-hidden values will continue to stay hidden.\n */\nfunction createAriaHider(dialogNode: HTMLElement): () => void {\n  const originalValues: any[] = []\n  const rootNodes: HTMLElement[] = []\n  const ownerDocument = getOwnerDocument(dialogNode) || document\n\n  if (!dialogNode) {\n    return noop\n  }\n\n  Array.prototype.forEach.call(\n    ownerDocument.querySelectorAll(\"body > *\"),\n    node => {\n      const portalNode = dialogNode.parentNode?.parentNode?.parentNode\n      if (node === portalNode) {\n        return\n      }\n      const attr = node.getAttribute(\"aria-hidden\")\n      const alreadyHidden = attr !== null && attr !== \"false\"\n      if (alreadyHidden) {\n        return\n      }\n      originalValues.push(attr)\n      rootNodes.push(node)\n      node.setAttribute(\"aria-hidden\", \"true\")\n    }\n  )\n\n  return () => {\n    rootNodes.forEach((node, index) => {\n      const originalValue = originalValues[index]\n      if (originalValue === null) {\n        node.removeAttribute(\"aria-hidden\")\n      } else {\n        node.setAttribute(\"aria-hidden\", originalValue)\n      }\n    })\n  }\n}\n\nexport default GenericModalContainer\n","/Users/ally.beaton/Code/kds/draft-packages/modal/KaizenDraft/Modal/Primitives/GenericModalSection.tsx",[],"/Users/ally.beaton/Code/kds/draft-packages/modal/KaizenDraft/Modal/Primitives/ModalAccessibleContext.ts",[],"/Users/ally.beaton/Code/kds/draft-packages/modal/KaizenDraft/Modal/Primitives/ModalAccessibleDescription.tsx",[],"/Users/ally.beaton/Code/kds/draft-packages/modal/KaizenDraft/Modal/Primitives/ModalAccessibleLabel.tsx",[],"/Users/ally.beaton/Code/kds/draft-packages/modal/KaizenDraft/Modal/Primitives/ModalFooter.tsx",[],"/Users/ally.beaton/Code/kds/draft-packages/modal/KaizenDraft/Modal/Primitives/ModalHeader.tsx",[],"/Users/ally.beaton/Code/kds/draft-packages/modal/KaizenDraft/Modal/index.ts",[],"/Users/ally.beaton/Code/kds/draft-packages/modal/index.ts",[],"/Users/ally.beaton/Code/kds/draft-packages/popover/KaizenDraft/Popover/Popover.tsx",[],"/Users/ally.beaton/Code/kds/draft-packages/popover/KaizenDraft/Popover/index.ts",[],"/Users/ally.beaton/Code/kds/draft-packages/popover/index.ts",[],"/Users/ally.beaton/Code/kds/draft-packages/radio/KaizenDraft/Radio/Primitives/RadioInput.spec.tsx",[],"/Users/ally.beaton/Code/kds/draft-packages/radio/KaizenDraft/Radio/Primitives/RadioInput.tsx",[],"/Users/ally.beaton/Code/kds/draft-packages/radio/KaizenDraft/Radio/Radio.spec.tsx",[],"/Users/ally.beaton/Code/kds/draft-packages/radio/KaizenDraft/Radio/Radio.tsx",[],"/Users/ally.beaton/Code/kds/draft-packages/radio/KaizenDraft/Radio/index.ts",[],"/Users/ally.beaton/Code/kds/draft-packages/radio/index.ts",[],"/Users/ally.beaton/Code/kds/draft-packages/radio-group/KaizenDraft/RadioGroup/RadioGroup.spec.tsx",[],"/Users/ally.beaton/Code/kds/draft-packages/radio-group/KaizenDraft/RadioGroup/RadioGroup.tsx",[],"/Users/ally.beaton/Code/kds/draft-packages/radio-group/KaizenDraft/RadioGroup/index.ts",[],"/Users/ally.beaton/Code/kds/draft-packages/radio-group/index.ts",[],"/Users/ally.beaton/Code/kds/draft-packages/select/KaizenDraft/Select/Select.tsx",[],"/