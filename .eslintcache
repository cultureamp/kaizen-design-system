\n    <GuidanceBlock\n      img={{ src: guidanceBlockImg, alt: \"Guidance block\" }}\n      text={guidanceBlockText}\n      actions={{\n        primary: {\n          label: \"Action\",\n          onClick: () => {\n            alert(\"tada: \")\n          },\n        },\n        dismiss: {\n          onClick: () => alert(\"tada: \"),\n        },\n      }}\n    />\n  </div>\n)\nconst WithoutActionArrowButton = () => (\n  <div style={{ display: \"flex\", justifyContent: \"center\", margin: \"100px\" }}>\n    <GuidanceBlock\n      img={{ src: guidanceBlockImg, alt: \"Guidance block\" }}\n      text={guidanceBlockText}\n      actions={{\n        primary: {\n          label: \"Action\",\n          onClick: () => {\n            alert(\"tada: \")\n          },\n        },\n        dismiss: {\n          onClick: () => alert(\"tada: \"),\n        },\n      }}\n      withActionButtonArrow={false}\n    />\n  </div>\n)\n\nconst Persistent = () => (\n  <div style={{ display: \"flex\", justifyContent: \"center\", margin: \"100px\" }}>\n    <GuidanceBlock\n      img={{ src: guidanceBlockImg, alt: \"Information illustration\" }}\n      text={guidanceBlockText}\n      actions={{\n        primary: {\n          label: \"Action\",\n          onClick: () => {\n            alert(\"tada: \")\n          },\n        },\n      }}\n      persistent\n    />\n  </div>\n)\n\nconst SecondaryAction = () => (\n  <div style={{ display: \"flex\", justifyContent: \"center\", margin: \"100px\" }}>\n    <GuidanceBlock\n      img={{ src: guidanceBlockImg, alt: \"Information illustration\" }}\n      text={guidanceBlockText}\n      actions={{\n        primary: {\n          label: \"Action\",\n          onClick: () => {\n            alert(\"tada: \")\n          },\n        },\n        secondary: {\n          label: \"Secondary action\",\n          onClick: () => alert(\"tada: \"),\n        },\n      }}\n      persistent\n      withActionButtonArrow={false}\n    />\n  </div>\n)\n\nDefault.story = {\n  name: \"Default\",\n  parameters: {\n    backgrounds: [\n      {\n        name: \"Stone\",\n        value: colorTokens.kz.color.stone,\n        default: true,\n      },\n    ],\n  },\n}\n\nPersistent.story = {\n  name: \"Persistent\",\n  parameters: {\n    backgrounds: [\n      {\n        name: \"Stone\",\n        value: colorTokens.kz.color.stone,\n        default: true,\n      },\n    ],\n  },\n}\n\nWithoutActionArrowButton.story = {\n  name: \"Without Action Arrow Button\",\n  parameters: {\n    backgrounds: [\n      {\n        name: \"Stone\",\n        value: colorTokens.kz.color.stone,\n        default: true,\n      },\n    ],\n  },\n}\n\nSecondaryAction.story = {\n  name: \"Secondary Action\",\n  parameters: {\n    backgrounds: [\n      {\n        name: \"Stone\",\n        value: colorTokens.kz.color.stone,\n        default: true,\n      },\n    ],\n  },\n}\n\nexport { Default, WithoutActionArrowButton, Persistent, SecondaryAction }\n","/Users/ally.beaton/Code/kds/draft-packages/stories/HeroCard.stories.tsx",[],"/Users/ally.beaton/Code/kds/draft-packages/stories/HeroPanel.stories.tsx",[],"/Users/ally.beaton/Code/kds/draft-packages/stories/IconButton.stories.tsx",[],"/Users/ally.beaton/Code/kds/draft-packages/stories/IllustrationScene.stories.tsx",[],"/Users/ally.beaton/Code/kds/draft-packages/stories/IllustrationSpot.stories.tsx",[],"/Users/ally.beaton/Code/kds/draft-packages/stories/LoadingPlaceholder.stories.tsx",[],"/Users/ally.beaton/Code/kds/draft-packages/stories/Menu.stories.tsx",["1538"],"import { Box, Paragraph } from \"@kaizen/component-library\"\nimport * as colorTokens from \"@kaizen/design-tokens/tokens/color.json\"\nimport { Button, IconButton } from \"@kaizen/draft-button\"\nconst chevronDown = require(\"@kaizen/component-library/icons/chevron-down.icon.svg\")\n  .default\nconst chevronUp = require(\"@kaizen/component-library/icons/chevron-up.icon.svg\")\n  .default\nconst duplicateIcon = require(\"@kaizen/component-library/icons/duplicate.icon.svg\")\n  .default\nconst editIcon = require(\"@kaizen/component-library/icons/edit.icon.svg\")\n  .default\nconst trashIcon = require(\"@kaizen/component-library/icons/trash.icon.svg\")\n  .default\nconst kebabIcon = require(\"@kaizen/component-library/icons/kebab.icon.svg\")\n  .default\nconst meatballsIcon = require(\"@kaizen/component-library/icons/meatballs.icon.svg\")\n  .default\nimport { action } from \"@storybook/addon-actions\"\nimport React, { useState } from \"react\"\nimport {\n  Menu,\n  MenuContent,\n  MenuHeader,\n  MenuItem,\n  MenuSeparator,\n  StatelessMenu,\n} from \"../menu\"\n\nconst StoryWrapper = ({ children }) => (\n  <div style={{ display: \"flex\", justifyContent: \"center\", marginTop: \"1rem\" }}>\n    <div style={{ display: \"inline-block\" }}>{children}</div>\n  </div>\n)\n\nconst MenuInstance: React.FunctionComponent = () => (\n  <MenuContent>\n    <MenuItem action=\"https://www.cultureamp.com/\" label=\"Menu link\" />\n    <MenuItem action=\"https://www.cultureamp.com/\" label=\"Menu link\" />\n    <MenuItem action=\"https://www.cultureamp.com/\" label=\"Menu link\" />\n    <MenuSeparator />\n    <MenuHeader title=\"Other actions\" />\n    <MenuItem\n      action={(e: any) => {\n        e.preventDefault()\n        action(\"I am an onClick handler\")()\n      }}\n      icon={duplicateIcon}\n      label=\"Menu button but the label is too long\"\n    />\n    <MenuItem\n      action={(e: any) => {\n        e.preventDefault()\n        action(\"I am an onClick handler\")()\n      }}\n      icon={editIcon}\n      label=\"Menu button\"\n    />\n\n    <MenuItem\n      action=\"https://www.cultureamp.com/\"\n      icon={trashIcon}\n      destructive\n      label=\"Menu button\"\n    />\n    <MenuItem\n      action=\"https://www.cultureamp.com/\"\n      disabled\n      label=\"Menu button\"\n    />\n  </MenuContent>\n)\n\nexport default {\n  title: \"Menu (React)\",\n  component: Menu,\n  parameters: {\n    info: {\n      text: `\n        import { Menu, MenuHeader, MenuItem, MenuContent, MenuSeparator } from \"@kaizen/draft-menu\";\n      `,\n    },\n  },\n}\n\nexport const LabelAndIcon = () => (\n  <StoryWrapper>\n    <Menu\n      button={<Button label=\"Label\" icon={chevronDown} iconPosition=\"end\" />}\n      dropdownId=\"dropdown\"\n    >\n      <MenuInstance />\n    </Menu>\n  </StoryWrapper>\n)\n\nLabelAndIcon.story = {\n  name: \"Label and Icon (Kaizen Site Demo)\",\n}\n\nexport const LabelAndIconReversed = () => (\n  <StoryWrapper>\n    <Menu\n      button={\n        <Button label=\"Label\" icon={chevronDown} iconPosition=\"end\" reversed />\n      }\n    >\n      <MenuInstance />\n    </Menu>\n  </StoryWrapper>\n)\n\nLabelAndIconReversed.story = {\n  name: \"Label and Icon (reversed)\",\n  parameters: {\n    backgrounds: [\n      {\n        name: \"Wisteria 700\",\n        value: colorTokens.kz.color.wisteria[700],\n        default: true,\n      },\n    ],\n  },\n}\n\nexport const DefaultMeatball = () => (\n  <StoryWrapper>\n    <Menu button={<IconButton label=\"\" icon={meatballsIcon} />} align=\"right\">\n      <MenuInstance />\n    </Menu>\n  </StoryWrapper>\n)\n\nDefaultMeatball.story = {\n  name: \"Default (Meatball)\",\n}\n\nexport const DefaultKebab = () => (\n  <StoryWrapper>\n    <Menu button={<IconButton label=\"\" icon={kebabIcon} />} align=\"right\">\n      <MenuInstance />\n    </Menu>\n  </StoryWrapper>\n)\n\nDefaultKebab.story = {\n  name: \"Default (Kebab)\",\n}\n\nexport const LabelAndIconBottom = () => (\n  <StoryWrapper>\n    <div style={{ marginTop: \"400px\" }}></div>\n    <Menu\n      button={<Button label=\"Label\" icon={chevronDown} iconPosition=\"end\" />}\n    >\n      <MenuInstance />\n    </Menu>\n  </StoryWrapper>\n)\n\nLabelAndIconBottom.story = {\n  name: \"Label and Icon (bottom of screen)\",\n  parameters: {\n    viewport: { defaultViewport: \"mobile1\" },\n  },\n}\n\nconst StatelessMenuExample = props => {\n  const [isMenuVisible, setIsMenuVisible] = useState(false)\n\