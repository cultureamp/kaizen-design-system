alue }}\n    >\n      <span style={textStyles}>{name}</span>\n      <span style={textStyles}>{value}</span>\n    </div>\n  )\n}\n\nexport default ZenColor\n","/Users/ally.beaton/Code/kds/site/src/docs-components/color/ZenColorGroup.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/color/ZenCoral.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/color/ZenNeutrals.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/color/ZenPeach.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/color/ZenSeedling.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/color/ZenWisteria.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/color/ZenYuzu.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/icons/IconGrid.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/icons/IconSizeSpaceTouch.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/icons/IconTile.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/icons/InteractionStates.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/icons/InteractionStatesDemo.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/localization/CAMarginExample.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/localization/CAPaddingExample.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/localization/CAPositionExample.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/localization/CATypeAlignExample.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/localization/LocalizationMixinImportExample.tsx",[],"/Users/ally.beaton/Code/kds/site/src/docs-components/localization/RTLLayoutExample.tsx",[],"/Users/ally.beaton/Code/kds/site/src/pages/404.tsx",[],"/Users/ally.beaton/Code/kds/site/src/pages/components.tsx",[],"/Users/ally.beaton/Code/kds/site/src/pages/guidelines.tsx",[],"/Users/ally.beaton/Code/kds/site/src/pages/index.tsx",[],"/Users/ally.beaton/Code/kds/site/src/pages/status.tsx",[],"/Users/ally.beaton/Code/kds/site/src/pages/storybook.tsx",[],"/Users/ally.beaton/Code/kds/site/src/templates/componentPage.tsx",["1558"],"import { graphql } from \"gatsby\"\nimport { MDXRenderer } from \"gatsby-plugin-mdx\"\nimport * as React from \"react\"\nimport ContentMarkdownSection from \"../components/ContentMarkdownSection\"\nimport Footer from \"../components/Footer\"\nimport Layout from \"../components/Layout\"\nimport PageHeader from \"../components/PageHeader\"\nimport {\n  Content,\n  ContentNeedToKnowSection,\n  Sidebar,\n  SidebarAndContent,\n  SidebarSection,\n  SidebarTab,\n} from \"../components/SidebarAndContent\"\nimport StorybookDemo from \"../components/StorybookDemo\"\nimport { sortSidebarTabs, stripTrailingSlash } from \"./util\"\n\nconst renderSidebarTabs = (pages, currentPath) =>\n  pages.map((node, i) => (\n    <SidebarTab\n      href={node!.node!.fields!.slug}\n      active={\n        stripTrailingSlash(node!.node!.fields!.slug) ===\n        stripTrailingSlash(currentPath)\n      }\n      key={`sidebarTab-${i}`}\n    >\n      {node!.node!.frontmatter!.navTitle}\n    </SidebarTab>\n  ))\n\nexport default ({ data, pageContext, location }) => {\n  const md = data.mdx\n  const allPages = data.allMdx.edges\n  const currentPath = location.pathname\n\n  const overviewPage = allPages.filter(\n    el => el.node.frontmatter.navTitle === \"Overview\"\n  )\n  const pagesWithoutOverview = sortSidebarTabs(\n    allPages.filter(el => el.node.frontmatter.navTitle !== \"Overview\")\n  )\n\n  const renderStorybookIFrame = () => {\n    if (!md.frontmatter.demoStoryId) {\n      // eslint-disable-next-line no-console\n      console.warn(\n        `Could not find a demo story ID for \"${md.frontmatter.title}\". Please make sure there is a frontmatter field called demoStoryId in the component docs. The ID comes from the Storybook URL for a given story.`\n      )\n      return undefined\n    }\n    return (\n      <StorybookDemo\n        demoId={md.frontmatter.demoStoryId}\n        demoHeight={md.frontmatter.demoStoryHeight}\n      />\n    )\n  }\n\n  const ComponentPageHeader = (\n    <PageHeader\n      headingText={md.frontmatter.title}\n      summaryParagraph={md.frontmatter.summaryParagraph}\n      tags={md.frontmatter.tags}\n      headerImageName={md.frontmatter.headerImage}\n    />\n  )\n\n  return (\n    <Layout\n      pageTitle={md.frontmatter.title}\n      currentPath={currentPath}\n      pageHeader={ComponentPageHeader}\n      footer={<Footer />}\n    >\n      <SidebarAndContent>\n        <Sidebar>\n          <SidebarSection>\n            {renderSidebarTabs(overviewPage, currentPath)}\n            {renderSidebarTabs(pagesWithoutOverview, currentPath)}\n          </SidebarSection>\n        </Sidebar>\n        <Content>\n          <ContentNeedToKnowSection listOfTips={md.frontmatter.needToKnow} />\n          {md.frontmatter.title !== \"Overview\" && renderStorybookIFrame()}\n          <ContentMarkdownSection>\n            <h1>{md.frontmatter.navTitle}</h1>\n            {/*\n            // @ts-ignore */}\n            <MDXRenderer>{data.mdx.body}</MDXRenderer>\n          </ContentMarkdownSection>\n        </Content>\n      </SidebarAndContent>\n    </Layout>\n  )\n}\n\nexport const query = graphql`\n  query($slug: String!) {\n    allMdx(filter: { fields: { slug: { regex: \"/^/components//\" } } }) {\n      edges {\n        node {\n          fields {\n            slug\n          }\n          frontmatter {\n            navTitle\n            title\n          }\n        }\n      }\n    }\n    mdx(fields: { slug: { eq: $slug } }) {\n      body\n      frontmatter {\n        title\n        tags\n        needToKnow\n        summaryParagraph\n        headerImage\n        demoStoryId\n        demoStoryHeight\n      }\n    }\n  }\n`\n","/Users/ally.beaton/Code/kds/site/src/templates/genericPage.tsx",[],"/Users/ally.beaton/Code/kds/site/src/templates/guidelinePage.tsx",[],"/Users/ally.beaton/Code/kds/site/src/templates/languagePage.t