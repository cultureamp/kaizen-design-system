    "build:watch": "yarn clean && yarn prepublish --watch",
    "clean": "rimraf '**/*.d.ts' '**/*.js' '**/*.map'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cultureamp/kaizen-design-system.git",
    "directory": "draft-packages/illustration"
  },
  "bugs": {
    "url": "https://github.com/cultureamp/kaizen-design-system/issues"
  },
  "files": [
    "**/*",
    "!**/*.ts",
    "!**/*.tsx",
    "**/*.d.ts",
    "!**/*.spec.*",
    "!**/*.snap",
    "!draft-templates",
    "!stories",
    "!docs",
    "!tsconfig.dist.json"
  ],
  "author": "",
  "private": false,
  "license": "MIT",
  "dependencies": {
    "jszip": "^3.2.2",
    "lottie-react": "^2.1.0",
    "@kaizen/hosted-assets": "^1.1.1"
  },
  "devDependencies": {
    "elm-storybook": "cultureamp/elm-storybook#0.3.0",
