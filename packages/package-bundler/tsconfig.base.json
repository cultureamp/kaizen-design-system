{
  "compilerOptions": {
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "jsx": "react",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "strict": true
  }
}
